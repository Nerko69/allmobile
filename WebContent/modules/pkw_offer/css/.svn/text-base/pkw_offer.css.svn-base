#main {
    padding: 2em;
}

#basic, #extended, #misc  {
    /*border: medium black ridge;*/
    background : #EEEEF3;
    padding-bottom: 2em;
    padding-left: 0.2em;
    -moz-box-shadow: 5px 5px 5px #ccc;
    -webkit-box-shadow: 5px 5px 5px #ccc;
    box-shadow: 5px 5px 5px #ccc;
}

#extended div {
    /*padding: 0.2em;*/
}
.separator {
    background: #dbdec8;
    border-bottom: 1px solid #A4B640;
    height: 1px;
    margin-top: 5px;
    margin-bottom: 5px;
}

.panel_header {
    background:url(img/aqua-hd-bg.gif) repeat-x;
    width : 100%;
    height: 22px;
    border-top-left-radius: 10px 5px;
    border-top-right-radius: 10px 5px;
    -moz-border-radius-topleft: 10px 5px;
    -moz-border-radius-topright: 10px 5px;
}

#main_panel {
    background: white;
    padding: 0.8em;
}

.title {
    font-size: 20px;
    font: bold;
    color: #3E0A12;
    margin-bottom: 0.5em;
    text-shadow: 0 0 0.2em #8B95B0, 0 0 0.2em #8B95B0, 0 0 0.2em #8B95B0;
}

.title2 {
    font-size: 20px;
    font: bold;
    /*margin-bottom: 0.5em;*/
    margin: 0.5em;
    margin-bottom: 0.1em;
}
#price {
    font-size: 15px;
    font: bold;
    text-shadow: threedshadow;
}

.parSeparator {
    height: 1em;
    width: 100%;
}

.titleUnderline {
    background: #111833;
    border-bottom: 1px dotted #475380;
    height: 1px;
    margin-bottom: 1em;
    margin-left: 0.5em;
}

.col1, .col2, .col3 {
    padding-top: 0.5em;
    padding-left: 0.2em;
    padding-right: 0.2em;
    text-align: left;
}

.entry {
    font-size: 14px;
    padding-bottom: 0.1em;
    padding-left: 0.5em;
    padding-right: 0.5em;
}

.list {
    padding-left: 2em;
    padding-right: 2em;
}
.liItem {
    list-style-type: disc;
    padding-bottom: 0.2em;
    
 }
 
