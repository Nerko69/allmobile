<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
<form id="quicksellForm" method="get" action="modules/sell_PKW/sell_PKW.js">
    <div id="SellContent" class="yui3-g">
        <div class="yui3-u-1-2">
            <div class="vSpaceTop">
                <fieldset id="sellModelList" dir="rtl">
                    <legend><%= lang.getMessage("Title.Model")%></legend>
                </fieldset>
            </div>
            <div class="vSpaceTop">
                <div><label for="sellManudate"><%= lang.getMessage("Title.ManuDate")%></label></div>
                <input id="sellManudate" type="text" class="mfield">
            </div>
            <div class="vSpaceTop">
                <fieldset id="sellOiltypeList" dir="rtl">
                    <legend><%= lang.getMessage("Title.OilType")%></legend>
                </fieldset>
            </div>
            <div class="vSpaceTop">
                <fieldset id="sellCityList" dir="rtl">
                    <legend><%= lang.getMessage("Title.City")%></legend>
                </fieldset>
            </div>
        </div>
        
        
        <div class="yui3-u-1-2">
            <div class="vSpaceTop">
                <fieldset id="sellBrandList" dir="rtl">
                    <legend><%= lang.getMessage("Title.Brand")%></legend>
                </fieldset>
            </div>
            <div class="vSpaceTop">
                <div><label for="sellPrice" dir="rtl"><%= lang.getMessage("Title.Price")%></label></div>
                <input id="sellPrice" type="text" class="mfield">
            </div>
            <div class="vSpaceTop">
                <div><label for="sellMeter"><%= lang.getMessage("Title.MeterList")%></label></div>
                <input id="sellMeter" type="text" class="mfield">
            </div>
            <div class="vSpaceTop">
                <fieldset id="sellCountryList" dir="rtl">
                    <legend><%= lang.getMessage("Title.Country")%></legend>
                </fieldset>
            </div>
            <div class="vSpaceTop">
                <fieldset id="sellStatusList" dir="rtl">
                    <legend><%= lang.getMessage("Title.Status")%></legend>
                </fieldset>
            </div>
            <div>
                <fieldset id="quickSellBtnDiv">
                    <legend></legend>
                </fieldset>
            </div>

        </div>
    </div>
</form>